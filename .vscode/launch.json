    "version": "0.2.0",
    "configurations": [
    {
      "name": "Py: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Py: Train",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "args": [
        "--config=configs/maml/bandit/bandit-k5-n10.yaml",
        "--output-folder=maml-banditk5n10",
        "--seed=1",
        "--num-workers=1"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Py: Test",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "args": [
        "--config=maml-banditk5n10/config.json",
        "--policy=maml-banditk5n10/policy.th",
        "--output=maml-banditk5n10/results.npz",
        "--meta-batch-size=20",
        "--num-batches=10",
        "--num-workers=1"
      ],
      "console": "integratedTerminal"
    }
    ]
}